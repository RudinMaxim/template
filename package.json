{
  "name": "template_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./scripts/copy-env.js && next dev",
    "build": "next build",
    "start": "node ./scripts/copy-env.js && next start",
    "lint": "next lint",
    "lint:style": "stylelint \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "create:page": "node ./scripts/page/create.js",
    "remove:page": "node ./scripts/page/remove.js",
    "create:ui": "node ./scripts/ui/create.js",
    "remove:ui": "node ./scripts/ui/remove.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "gsap": "^3.12.5",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
