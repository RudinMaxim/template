{
  "name": "template_front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./scripts/copy-env.js && next dev",
    "build": "next build",
    "start": "node ./scripts/copy-env.js && next start",
    "lint": "next lint",
    "lint:style": "stylelint \"**/*.scss\"",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "format": "prettier --write .",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "create:page": "node ./scripts/page/create.js",
    "remove:page": "node ./scripts/page/remove.js",
    "create:ui": "node ./scripts/ui/create.js",
    "remove:ui": "node ./scripts/ui/remove.js",
    "create:feature": "node ./scripts/feature/create.js",
    "remove:feature": "node ./scripts/feature/remove.js",
    "create:entity": "node ./scripts/entity/create.js",
    "remove:entity": "node ./scripts/entity/remove.js",
    "git-flow": "node ./scripts/git-flow.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "gsap": "^3.12.5",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^3.3.3",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  }
}
